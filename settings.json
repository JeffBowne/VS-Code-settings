{
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.hover.delay": 800,
  "editor.scrollBeyondLastLine": false,
  "editor.cursorBlinking": "smooth",
  "editor.minimap.maxColumn": 80,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.minimap.scale": 2,
  "editor.minimap.side": "right",
  "editor.minimap.size": "fit",
  "editor.folding": false,
  "editor.matchBrackets": "never",
  "editor.renderLineHighlight": "none",
  "editor.suggestFontSize": 12,
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 22,
  "editor.lineHeight": 24,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    80, 100
  ],
  "workbench.editor.focusRecentEditorAfterClose": false,
  "workbench.commandPalette.history": 80,
  "workbench.commandPalette.preserveInput": true,
  "workbench.tree.indent": 12,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.colorTheme": "One Dark Pro Monokai Darker",
  "workbench.colorCustomizations": {
    "[One Dark Pro Monokai Darker]": {
      "sideBar.background": "#000000",
      "tab.activeBackground": "#2f333b",
      "tab.activeModifiedBorder": "#ff9a3c",
      "tab.inactiveBackground": "#000000",
      "tab.inactiveForeground": "#757575",
      "tab.border": "#3B3B3B",
      "editorHoverWidget.border": "#757575",
      "editorRuler.foreground": "#3B3B3B",
      "editorGutter.background": "#1D1F23",
      "editorLineNumber.foreground": "#4B4B4B",
      "editorLineNumber.activeForeground": "#ffffff",
      "editorWarning.foreground": "#4B4B4B",
      "breadcrumb.background": "#2f333b",
      "editor.selectionBackground": "#737d96",
      "editor.wordHighlightBackground": "#75757550",
      "editor.selectionHighlightBorder": "#9199af",
      "editor.selectionHighlightBackground": "#00000000",
      "editor.findMatchBorder": "#eeeeee",
      "editor.findMatchBackground": "#000000",
      "editor.findMatchHighlightBorder": "#9199af",
      "editor.findMatchHighlightBackground": "#00000000",
      "list.inactiveSelectionBackground": "#4B4B4B",
      "list.activeSelectionBackground": "#2f333b"
    }
  },
  "window.restoreWindows": "all",
  "window.title": "${activeFolderShort}${separator}${activeEditorShort}",
  "window.openFilesInNewWindow": "default",
  "explorer.decorations.badges": false,
  "explorer.openEditors.visible": 0,
  "files.eol": "\n",
  "emmet.triggerExpansionOnTab": true,
  "emmet.syntaxProfiles": {
    "html": {
      "attr_quotes": "double"
    }
  },
  "errorLens.gutterIconsEnabled": true,
  "errorLens.gutterIconSet": "circle",
  "errorLens.warningForeground": "#4B4B4B",
  "errorLens.warningBackground": "",
  "errorLens.errorBackground": "",
  "errorLens.errorForeground": "#4B4B4B",
  "errorLens.hintBackground": "",
  "errorLens.hintForeground": "#4B4B4B",
  "errorLens.infoBackground": "",
  "errorLens.infoForeground": "#4B4B4B",
  "errorLens.margin": "80px",
  "errorLens.gutterIconSize": "80%",
  "errorLens.fontSize": "12px",
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "bracket-pair-colorizer-2.colors": ["#c6cfdf"],
  "turboConsoleLog.logMessagePrefix": "HEY LOOK ",
  "trailing-spaces.deleteModifiedLinesOnly": true,
  "scss.lint.emptyRules": "ignore",
  "stylint.stylintrcPath": "./.stylelintrc",
  "diffEditor.ignoreTrimWhitespace": true,
  "trailing-spaces.backgroundColor": "rgba(155,0,0,0.5)",
  "trailing-spaces.borderColor": "rgba(155,100,100,0.15)",
  "stylint.packageManager": "yarn",
  "stylint.run": "onSave",
  "turboConsoleLog.addSemicolonInTheEnd": true,
  "turboConsoleLog.quote": "'",
  "html.autoClosingTags": false,
  "customizeUI.stylesheet": {
    ".editor-actions": "display: none !important;",
  },
  "customizeUI.activityBar": "bottom",
  "customizeUI.titleBar": "inline",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": false
  },
  "window.zoomLevel": 0,
  "[scss]": {},
  "emmet.showAbbreviationSuggestions": false,
  "gitlens.hovers.currentLine.over": "line",
  "window.titleBarStyle": "native",
  "extensions.ignoreRecommendations": false,
}
